{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es5",
    "skipLibCheck": true,
    "noEmit": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}
